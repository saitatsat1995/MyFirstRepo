
@{
    ViewBag.Title = "ALL_TRACKS";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
     <div class="row">
          <table class="table table-striped">
              <tr>
                  <th>Track ID</th>
                  <th>Track Name</th>
                  <th>Lead Name</th>
                  <th>Room Name</th>
              </tr>
              @foreach (var i in ViewBag.trackList) {
                  <tr>
                      <td>@Html.Encode(i.Track_ID)</td>
                      <td>@Html.Encode(i.Track_Name)</td>
                      <td>@Html.Encode(i.LeadName)</td>
                      <td>@Html.Encode(i.RoomAllocated)</td>
                  </tr>
              }
          </table>
     </div>
</div>

