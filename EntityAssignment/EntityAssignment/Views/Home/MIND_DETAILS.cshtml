
@{
    ViewBag.Title = "MIND_DETAILS";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container" style="margin-top:100px">
    <div class="row">
        <div class="col-md-6">
            <h5><center><b>MIND DETAILS</b></center></h5>
            <table class="table table-striped">
                <tr>
                    <th>Name</th>
                    <td>@Html.Encode(ViewBag.mindDetails.Name)</td>
                </tr>
                <tr>
                    <th>Gender</th>
                    <td>@Html.Encode(ViewBag.mindDetails.Gender)</td>
                </tr>
                <tr>
                    <th>Contact Number</th>
                    <td>@Html.Encode(ViewBag.mindDetails.ContactNumber)</td>
                </tr>
                <tr>
                    <th>MID</th>
                    <td>@Html.Encode(ViewBag.mindDetails.MID)</td>
                </tr>
            </table>
        </div>
        <div class="col-md-6">
            <h5><center><b>TRACK DETAILS</b></center></h5>
            <table class="table table-striped">
                <tr>
                    <th>TRACK ID</th>
                    <td>@Html.Encode(ViewBag.trackDetails.Track_ID)</td>
                </tr>
                <tr>
                    <th>NAME</th>
                    <td>@Html.Encode(ViewBag.trackDetails.Track_Name)</td>
                </tr>
                <tr>
                    <th>ROOM ALLOCATED</th>
                    <td>@Html.Encode(ViewBag.trackDetails.RoomAllocated)</td>
                </tr>
                <tr>
                    <th>LEAD NAME</th>
                    <td>@Html.Encode(ViewBag.trackDetails.LeadName)</td>
                </tr>
            </table>
        </div>
    </div>
    <center><a href="/Home/ALL_MINDS"><button class="btn btn-success"><-&nbsp;Back</button></a></center>
</div>

